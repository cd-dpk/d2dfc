<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:gravity="center"
    android:orientation="vertical"
    style="@style/AppTheme.GrayColorPrimary"
    tools:context=".pages.AboutDevInfoActivity">
    <include
        android:id="@+id/member_info_bio"
        layout="@layout/card_member_bio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"/>
    <include
        android:id="@+id/member_info_travel"
        layout="@layout/card_member_travel_history"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"/>
    <include
        android:id="@+id/member_info_health"
        layout="@layout/card_member_health_history"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"/>
    <include
        android:id="@+id/member_info_health"
        layout="@layout/card_member_respirotory"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"/>
    <include
        android:id="@+id/member_info_corona_recent"
        layout="@layout/card_member_corona_related_history"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="1"/>





</LinearLayout>